{
	"svelte.enable-ts-plugin": true,
	"search.exclude": {
		"**/node_modules": true,
		"**/.svelte-kit": true,
		"**/build": true,
		"**/pnpm-lock.yaml": true,
		"**/target": true,
		"**/docs": true,
		"**/locales": true,
		"CHANGELOG.md": true,
		"llms-full.txt": true
	},
	"sonarlint.connectedMode.project": {
		"connectionId": "seenivers",
		"projectKey": "Seenivers_App"
	},
	"[svelte]": {
		"editor.defaultFormatter": "svelte.svelte-vscode"
	},
	"html.customData": ["./node_modules/vidstack/vscode.html-data.json"],
	"i18n-ally.localesPaths": ["src/lib/i18n", "src/lib/i18n/locales"],
	"i18n-ally.keystyle": "nested",
	"i18n-ally.sortKeys": true,
	"i18n-ally.keepFulfilled": true,
	"i18n-ally.autoDetection": true,
	"i18n-ally.fullReloadOnChanged": true
}
